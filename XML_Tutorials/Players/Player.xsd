<?xml version = "1.0" encoding = "UTF-8" ?>

<xs:schema>
	<xs:element name = "players">
		<xs:complexType>
			<xs:sequence>
				<xs:element name = "playername" type = "xs:string"/>
				<xs:element name = "age" type = "xs:number"/>
				<xs:element name = "batting_avg" type = "xs:number"/>
				<xs:element name = "highest_score" type = "xs:number"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>	